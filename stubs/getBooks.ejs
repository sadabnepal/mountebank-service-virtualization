{
    "predicates": [
        {
            "equals": {
                "method": "GET",
                "path": "/books"
            }
        }
    ],
    "responses": [
        {
            "is": {
                "statusCode": 200,
                "headers": {
                    "Content-Type": "application/json"
                },
                "body": "<%- stringify(filename, "response/books.json") %>"
            }
        }
    ]
}